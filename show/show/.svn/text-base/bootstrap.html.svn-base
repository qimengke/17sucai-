		    <div class="col-sm-10">
		      <input type="password" class="form-control" id="inputEpassword4" placeholder="Email">
		    </div>
		  </div>
		  
		  <div class="form-group">
		    <div class="col-sm-offset-2 col-sm-10">
		      <button type="submit" class="btn btn-default">Sign in</button>
		    </div>
		  </div>
	</div>
</form>
